<script type="text/javascript">
$.ajax({
	type : "GET",
	async:false,
	url : "http://pdlib.pconline.com.cn/product/service/_product_info.jsp?pIds=555253",
	dataType : "jsonp",
	jsonp: "callbackparam",
	jsonpCallback:"success_jsonpCallback",
	success : function(json){
		alert(json);
		alert(json[0].id);
	},
	error:function(){
		alert('error');
	}
});
</script>
